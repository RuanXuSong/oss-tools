{
  "name": "spa-template",
  "version": "1.0.0",
  "description": "杭州雷数前端中后台管理系统模板",
  "license": "MIT",
  "author": "陈杰 <chenjie@thundersdata.com>",
  "homepage": "https://github.com/thundersdata-frontend/spa-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thundersdata-frontend/spa-template.git"
  },
  "bugs": {
    "url": "https://github.com/thundersdata-frontend/spa-template/issues"
  },
  "scripts": {
    "commit": "git-cz",
    "start": "umi dev",
    "build": "UMI_ENV=prod umi build",
    "test": "jest --passWithNoTests",
    "tsc": "tsc -p ./tsconfig.json",
    "eslint:fix": "eslint --fix --ext .ts,.tsx src/",
    "lint:fix": "prettier --check src/**/*.tsx --write"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "changelog": {
    "emojis": true,
    "authorName": true,
    "authorEmail": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tsc && npm run eslint:fix && npm run lint:fix && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "git add ."
    ]
  },
  "dependencies": {
    "@ice/store": "^0.3.0",
    "@sentry/browser": "^5.9.0",
    "@td-design/utils": "^2.1.7",
    "@td-design/web": "^2.2.1",
    "antd": "^3.25.1",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "lscache": "^1.3.0",
    "pont-engine": "0.3.4",
    "qs": "^6.9.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-use": "^13.6.2",
    "umi-plugin-polyfill": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-typescript": "^7.7.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@td-design/webpack-theme-replacer": "1.0.0",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.9.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.147",
    "@types/lscache": "^1.3.0",
    "@types/node": "^12.12.7",
    "@types/qs": "^6.9.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-test-renderer": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.27",
    "conventional-changelog-custom-config": "^0.2.0",
    "cz-customizable": "^6.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-flowtype": "^4.4.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "husky": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.9.0",
    "standard-version": "^7.0.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.7.2",
    "umi": "^2.11.3",
    "umi-plugin-react": "^1.13.3",
    "umi-types": "^0.5.4",
    "webpack-sentry-plugin": "^2.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
